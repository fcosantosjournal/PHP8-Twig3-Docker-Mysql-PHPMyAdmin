{% for item in routes %}
  {% for key, route in item %}
    {% if route != '404' and key != 'template' and key != 'controller' %}

      {% if path == route and logged == false %}
        <a href="register">Cadastrar</a><br>
        
      {% elseif route != 'login' and route != 'logout' and logged == false and route != 'register' %}
        <a href="{{ route }}">{{ route }}</a><br>  

      {% elseif route == 'login' and path != 'login' and logged == false and route != 'register' %}
        <a href="login">Login</a><br>

      {% elseif route != 'login' and path != 'login' and logged == true and route != 'register' %}
        <a href="{{ route }}">{{ route }}</a><br>
      {% endif %}
      
    {% endif %}
  {% endfor %}
{% endfor %}


